		  "lib": ["./src/lib/index.tsx"]
		}
	  },
	"dependencies": {
		"@kobalte/core": "^0.12.3",
		"@solidjs/router": "^0.13.0",
		"@tanstack/solid-form": "^0.13.7",
		"@tanstack/zod-form-adapter": "^0.13.7",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"solid-icons": "^1.1.0",
		"solid-js": "^1.8.15",
		"tailwind-merge": "^2.2.1",
		"zod": "^3.22.4"
	}
}
