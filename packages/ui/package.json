{
	"name": "@mattrax/ui",
	"description": "Mattrax's UI component library. Based on ShadCN/Solid UI components",
	"type": "module",
	"private": true,
	"sideEffects": false,
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./forms": "./src/forms/index.tsx",
		"./lib": "./src/lib/index.ts",
